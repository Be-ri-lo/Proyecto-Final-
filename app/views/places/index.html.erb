<p id="notice"><%= notice %></p>

<h1>Places</h1>

<table id="places">
  <thead>
    <tr>
      <th>Country</th>
      <th>City</th>
      <th>Location</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @places.each do |place| %>
        <%= render 'place', place: place %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>
